package poker;

import java.util.Random;
import java.util.Scanner;

public class BetMoney implements Bet
{
	private int bet;
	public BetMoney(int bet) 
	{
		this.bet = bet;
	}
	
	public boolean checkBet()
	{
		if(bet == 5 || bet == 10 || bet == 15)
			return true;
		return false;
	}
	
	public void setBet(int bet)
	{
		this.bet = bet;
	}
	
	public int getBet()
	{
		return bet;
	}

}
